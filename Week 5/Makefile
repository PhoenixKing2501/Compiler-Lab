ass5_20CS10063_20CS30057_output.txt: parser ass5_20CS10063_20CS30057_test.c
	./parser < ass5_20CS10063_20CS30057_test.c > ass5_20CS10063_20CS30057_output.txt

parser: ass5_20CS10063_20CS30057.yy.o ass5_20CS10063_20CS30057.tab.o ass5_20CS10063_20CS30057_main.o
	gcc -Wall ass5_20CS10063_20CS30057.yy.o ass5_20CS10063_20CS30057.tab.o ass5_20CS10063_20CS30057_main.o -o parser

ass5_20CS10063_20CS30057_main.o: ass5_20CS10063_20CS30057_main.c
	gcc -c -Wall ass5_20CS10063_20CS30057_main.c

ass5_20CS10063_20CS30057.yy.o: ass5_20CS10063_20CS30057.yy.c
	gcc -c -Wall ass5_20CS10063_20CS30057.yy.c

ass5_20CS10063_20CS30057.tab.o: ass5_20CS10063_20CS30057.tab.c
	gcc -c -Wall ass5_20CS10063_20CS30057.tab.c

ass5_20CS10063_20CS30057.yy.c: ass5_20CS10063_20CS30057.l ass5_20CS10063_20CS30057.tab.h
	flex --outfile=ass5_20CS10063_20CS30057.yy.c ass5_20CS10063_20CS30057.l

ass5_20CS10063_20CS30057.tab.c ass5_20CS10063_20CS30057.tab.h: ass5_20CS10063_20CS30057.y
	bison -d --debug --report=all ass5_20CS10063_20CS30057.y

clean:
	rm parser ass5_20CS10063_20CS30057_main.o ass5_20CS10063_20CS30057.yy.* ass5_20CS10063_20CS30057.tab.* ass5_20CS10063_20CS30057.output
